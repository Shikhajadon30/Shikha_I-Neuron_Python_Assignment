    "\n",
    "Answer:- \n",
    "from google.colab import files\n",
    "uploaded = files.upload()\n",
    "\n",
    "import zoo\n",
    "from importlib import reload\n",
    "reload(zoo)\n",
    "\n",
    "zoo.hours()"
